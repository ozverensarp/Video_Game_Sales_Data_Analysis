    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "import seaborn as sns\n",
    "import random\n",
    "\n",
    "# Assign random colors to each unique genre for better visualization\n",
    "genre_colors = {genre: sns.color_palette(\"husl\")[i % len(sns.color_palette(\"husl\"))] for i, genre in enumerate(genre_sales_user_score['Genre'].unique())}\n",
    "\n",
    "# Create a new column 'Color' in genre_sales_user_score to map each genre to its assigned color\n",
    "genre_sales_user_score['Color'] = genre_sales_user_score['Genre'].map(genre_colors)\n",
    "\n",
    "# Create a scatter plot with labeled points\n",
    "fig, ax = plt.subplots()\n",
    "\n",
    "# Iterate through rows to plot individual points with specified color and label for each genre\n",
    "for index, row in genre_sales_user_score.iterrows():\n",
    "    ax.scatter(row['User_Score'], row['Global_Sales'], c=row['Color'], label=row['Genre'], alpha=0.5)\n",
    "\n",
    "# Set labels and title\n",
    "ax.set_xlabel('User Score')\n",
    "ax.set_ylabel('Global Sales')\n",
    "ax.set_title('User Score vs Global Sales by Genre')\n",
    "\n",
    "# Add a legend to the plot to identify each genre by color\n",
    "ax.legend()\n",
    "\n",
    "# Display the plot\n",
    "plt.show()\n",
    "\n"
   ]
